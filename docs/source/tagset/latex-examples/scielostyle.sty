      \captionsetup{margin=10pt,
                  font=small,
                  labelfont=bf,
                  labelsep=endash}
      \newcommand{\imagewidthdefault}{0.5\textwidth}
      \newcommand{\xmlcaption}[2]{\caption{#2}}  %  <label>+<caption>



